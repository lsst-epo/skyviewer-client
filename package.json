{
  "name": "skyviewer-client",
  "version": "0.1.0",
  "author": "Rubin EPO",
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/lsst-epo/skyviewer-client"
  },
  "scripts": {
    "dev": "next dev",
    "static": "npm-run-all static:*",
    "static:build": "next build --debug",
    "static:export": "next export",
    "start": "next start -p 8080",
    "bs": "next build && next start -p 3000",
    "commitlint": "commitlint --edit",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "lint": "npm-run-all lint:*",
    "lint:js": "prettier --check \"**/*.{js,jsx}\" & eslint -c .eslintrc.js \"**/*.{js,jsx,ts,tsx}\"",
    "lint:scss": "stylelint --config .stylelintrc.js \"**/*.scss\"",
    "fix": "npm-run-all fix:js fix:scss fix:styled",
    "fix:js": "prettier --write \"**/*.{js,jsx}\" && eslint -c .eslintrc.js \"**/*.{js,jsx,ts,tsx}\" --fix",
    "fix:scss": "stylelint --config .stylelintrc.js \"**/*.scss\" --fix",
    "test": "vitest run",
    "release": "commit-and-tag-version",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "commit-and-tag-version": {
    "skip": {
      "commit": true
    }
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      [
        "babel-plugin-styled-components",
        {
          "ssr": true,
          "displayName": true
        }
      ]
    ]
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "lint-staged": {
    "*.{js,jsx}": "yarn fix:js",
    "*.scss": "yarn fix:scss",
    "**/styles.js": "yarn fix:styled"
  },
  "dependencies": {
    "@babel/runtime": "7.11.2",
    "@floating-ui/react-dom": "^2.1.2",
    "@headlessui/react": "2.0.4",
    "@rubin-epo/epo-react-lib": "^2.7.0",
    "@rubin-epo/epo-widget-lib": "^1.1.1",
    "@urql/next": "^1.1.1",
    "aladin-lite": "^3.6.1",
    "clsx": "^2.1.1",
    "file-saver": "^2.0.5",
    "focus-trap": "^6.1.2",
    "graphql": "^16.9.0",
    "graphql-request": "6",
    "gsap": "^3.12.7",
    "i18next": "23",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-resources-to-backend": "^1.2.1",
    "lodash": "^4.17.21",
    "motion": "^12.4.10",
    "next": "^14.2.25",
    "next-i18n-router": "^5.5.1",
    "next-view-transitions": "^0.3.4",
    "nextstepjs": "^2.0.0",
    "prettier": "^2.2.1",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "13",
    "react-icons": "^5.4.0",
    "react-share": "^5.2.2",
    "react-use-audio-player": "^4.0.1",
    "sass": "^1.77.8",
    "server-only": "^0.0.1",
    "sharp": "^0.33.5",
    "urql": "^4.1.0",
    "usehooks-ts": "^3.1.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@csstools/postcss-global-data": "^3.0.0",
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/client-preset": "4.3.3",
    "@rushstack/eslint-patch": "^1.11.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/file-saver": "^2.0.7",
    "@types/lodash": "^4.17.7",
    "@types/node": "^20.14.12",
    "@types/react": "18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-slider": "^1.3.6",
    "@typescript-eslint/parser": "^7.17.0",
    "@vitejs/plugin-react": "^4.3.4",
    "babel-plugin-styled-components": "^2.1.4",
    "commit-and-tag-version": "^12.4.1",
    "eslint": "^8.4.1",
    "eslint-config-next": "14",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^8.0.3",
    "jsdom": "^26.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-custom-media": "^11.0.5",
    "postcss-preset-env": "^9.6.0",
    "stylelint": "16",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-recess-order": "^6.0.0",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-scss": "^4.0.1",
    "typescript": "^5.5.4",
    "typescript-plugin-css-modules": "^5.1.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.9"
  },
  "resolutions": {
    "**/ansi-regex": "^5.0.1",
    "**/glob-parent": "^5.1.2",
    "styled-components": "^6.1.16"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
