{
  "name": "skyviewer-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel tokens dev:*",
    "dev:next": "next dev",
    "static": "run-s tokens static:*",
    "static:build": "next build",
    "static:export": "next export",
    "start": "next start",
    "tokens": "npx json-to-scss theme/tokens.json theme/styles/abstracts/_tokens.scss",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint -c .eslintrc.js \"**/*.{js,jsx}\"",
    "lint:scss": "stylelint --config .stylelintrc.js \"**/*.scss\" --syntax scss",
    "fix": "npm-run-all fix:js fix:scss",
    "fix:js": "eslint -c .eslintrc.js \"**/*.{js,jsx}\" --fix",
    "fix:scss": "stylelint --config .stylelintrc.js \"**/*.scss\" --syntax scss --fix",
    "fix:staged": "lint-staged"
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      [
        "styled-components",
        {
          "ssr": true
        }
      ]
    ]
  },
  "browserslist": [
    "defaults"
  ],
  "lint-staged": {
    "*.{js,jsx}": "yarn fix:js",
    "*.scss": "yarn fix:scss"
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.11.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-react-jsx": "^7.10.4",
    "@babel/runtime": "7.11.2",
    "@moxy/next-layout": "^2.2.3",
    "classnames": "^2.2.6",
    "copy-to-clipboard": "^3.3.1",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "focus-trap": "^6.1.2",
    "focus-visible": "^5.1.0",
    "hoist-non-react-statics": "^3.3.2",
    "i18next": "^19.8.2",
    "json-to-scss": "^1.6.2",
    "lodash.debounce": "^4.0.8",
    "netlify-plugin-cache-nextjs": "^1.5.2",
    "next": "^9.5.2",
    "npm-run-all": "^4.1.5",
    "postcss-normalize": "^9.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-i18next": "^11.7.3",
    "react-range": "^1.8.7",
    "sass": "^1.26.12",
    "striptags": "^3.1.1",
    "styled-components": "^5.2.0",
    "stylelint": "^13.8.0",
    "stylelint-config-recess-order": "^2.3.0",
    "stylelint-scss": "^3.18.0",
    "swr": "^0.3.5",
    "use-resize-observer": "^6.1.0"
  }
}
